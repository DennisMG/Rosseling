@{
    PageData["title"] = "UpdateAccount";
}

<div class="well"  >
    <ul class="nav nav-tabs">
        <li class="active"><a href="#home" data-toggle="tab">Profile</a></li>

    </ul>
    <div id="myTabContent" class="tab-content">
        <div class="tab-pane active in" id="home">
            <form id="tab">
                <br>
                <label>First Name</label><br>
                <input type="text" ng-model="UpdateAccountModel.FirstName" value="UpdateAccountModel.FirstName" class="form-control" placeholder="First Name"><br>
                <label>Last Name</label><br>
                <input type="text" ng-model="UpdateAccountModel.LastName" value="UpdateAccountModel.LastName" class="form-control" placeholder="Last Name"><br>
                <label>Email</label><br>
                <input type="email" ng-model="UpdateAccountModel.Email" value="UpdateAccountModel.Email" class="form-control" placeholder="Example: jsmith@email.com"><br>


                <div>
                    <button class="btn btn-primary" ng-click="UpdateAccount()">Update</button>
                </div>
            </form>
        </div>

    </div>
</div>


